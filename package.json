{
  "name": "@flyyer/stringify",
  "version": "0.1.2",
  "description": "Just a wrapper around https://github.com/streamich/fastest-stable-stringify",
  "keywords": [
    "flyyer"
  ],
  "main": "dist/stringify.es5.js",
  "module": "dist/stringify.esm.js",
  "typings": "dist/stringify.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "author": "@streamich",
  "repository": {
    "type": "git",
    "url": "https://github.com/useflyyer/flyyer-stringify.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c rollup.config.ts",
    "prepublishOnly": "yarn run build",
    "start": "rollup -c rollup.config.ts -w",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ]
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@flyyer/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.0",
    "eslint": "^7.30.0",
    "fastest-stable-stringify": "^2.0.2",
    "husky": "^4.3.7",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
